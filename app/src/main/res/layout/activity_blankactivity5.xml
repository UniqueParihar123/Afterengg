<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/blank5"
    android:background="@drawable/homeback"
    tools:context="com.example.anupamparihar.afterengg.blankactivity5">
    <RelativeLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <com.google.android.youtube.player.YouTubePlayerView
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:id="@+id/ypv"></com.google.android.youtube.player.YouTubePlayerView>


            <Button
                android:layout_width="330dp"
                android:layout_height="50dp"
                android:id="@+id/btn2"
                android:background="@drawable/logedittext"
                android:textColor="@color/btechbranchtext"
                android:textSize="20sp"
                android:text="   Play  "
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />
        </RelativeLayout>

    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
