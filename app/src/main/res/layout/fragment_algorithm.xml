<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/homeback"
    tools:context="com.example.anupamparihar.afterengg.algorithm">

    <!-- TODO: Update blank fragment layout -->

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <FrameLayout
            android:id="@+id/frame_fragment"
            android:layout_width="match_parent"
            android:layout_height="165dp"></FrameLayout>
        <android.support.v7.widget.CardView
            android:id="@+id/cva"
            android:layout_width="320dp"
            android:layout_height="140dp"

            app:cardCornerRadius="30dp"
            android:layout_above="@+id/cvb"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="21dp">


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="188dp"
                android:background="@drawable/homeback">

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="260dp"
                    android:layout_height="60dp"
                    android:textColor="@color/btechbranchtext"
                    android:background="@drawable/logedittext"
                    android:layout_alignEnd="@+id/tv1"
                    android:layout_alignRight="@+id/tv1"
                    android:layout_centerVertical="true"
                    android:layout_gravity="left"
                    android:gravity="center"
                    android:text="C Programming-In-Depth Exploration"

                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tv1"
                    android:layout_width="250dp"
                    android:layout_height="40dp"

                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="14dp"
                    android:text="DESIGNING ALGORITHM"
                    android:textColor="@color/btechbranchtext"
                    android:textSize="21sp" />
            </RelativeLayout>

        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:layout_width="320dp"
            android:layout_height="140dp"
            android:id="@+id/cvb"

            app:cardCornerRadius="30dp"
            android:layout_alignParentBottom="true"
            android:layout_alignLeft="@+id/cva"
            android:layout_alignStart="@+id/cva"
            android:layout_marginBottom="44dp">


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="188dp"
                android:background="@drawable/homeback">

                <TextView
                    android:id="@+id/tv3"
                    android:layout_width="260dp"
                    android:layout_height="60dp"
android:background="@drawable/logedittext"
                    android:layout_alignEnd="@+id/tv4"
                    android:layout_alignRight="@+id/tv4"
                    android:layout_centerVertical="true"
                    android:layout_gravity="left"
                    android:gravity="center"
                    android:text="Simple Sorting Algos Imprtant For GATE"
                    android:textColor="@color/btechbranchtext"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tv4"
                    android:layout_width="250dp"
                    android:layout_height="40dp"

                    android:layout_marginTop="12dp"
                    android:text="        SORTING        "
                    android:textColor="@color/btechbranchtext"
                    android:textSize="25sp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginRight="24dp"
                    android:layout_marginEnd="24dp" />
            </RelativeLayout>

        </android.support.v7.widget.CardView>
    </RelativeLayout>
</FrameLayout>
